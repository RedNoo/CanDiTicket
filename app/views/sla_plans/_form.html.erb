<% if @sla_plan.errors.any? %>
<ul>
<% @sla_plan.errors.full_messages.each do |msg| %>
<li><%= msg %></li>
<% end %>
</ul>
<% end %>
<%= form_for @sla_plan do |f| %>
  <p>
<%= f.label :title %><br/>
<%= f.text_field :title %>
  </p>
  <p>
<%= f.label :responsetime %><br/>
<%= f.text_field :responsetime %>
  </p>
  <p>
  <%= f.submit %>
</p>
<% end %>
<%=link_to "Back to sla_plan list", sla_plans_path %>
