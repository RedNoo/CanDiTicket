<table  class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>
        Title
      </th>
      <th>
        Actions
      </th>
    </tr>
  </thead>
  <tbody>
    <%obj.each do |ticket| %>
      <tr>
        <td>
          <%=link_to ticket.subject, ticket_path(ticket)%>
          <small>Created by:  <%=ticket.member.email if ticket.member%>, <%= time_ago_in_words(ticket.created_at)%> </small>
        </td>
        <td>
          <%=link_to "Edit", edit_ticket_path(ticket),class:"btn btn-default" %>
          <%=link_to "Show", ticket_path(ticket),class:"btn btn-info" %>
          <%=link_to "Delete", ticket_path(ticket), method: :delete, data: {confirm: "are you sure"} ,class:"btn btn-danger"%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
