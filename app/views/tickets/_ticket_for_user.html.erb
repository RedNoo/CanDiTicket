<table  class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>
        Title
      </th>
    </tr>
  </thead>
  <tbody>
    <%obj.each do |ticket| %>
      <tr>
        <td>
          <%=link_to ticket.subject, showticket_for_user_path(ticket)%>
          <small>Created by:  <%=ticket.member.email if ticket.member%>, <%= time_ago_in_words(ticket.created_at)%> </small>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
