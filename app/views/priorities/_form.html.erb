<div class="col-lg-6 col-md-6">
<% if @priority.errors.any? %>
<ul>
<% @priority.errors.full_messages.each do |msg| %>
<li><%= msg %></li>
<% end %>
</ul>
<% end %>
<%= form_for @priority do |f| %>
    <div class="form-group">
<%= f.label :title %><br/>
<%= f.text_field :title , class: "form-control"%>
  </div>
  <div class="form-group">
  <%= f.submit %>
</div>
<% end %>
<%=link_to "Back to priority list", priorities_path %>
</div>
